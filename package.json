{
    "name": "drop-farmer",
    "productName": "drop-farmer",
    "description": "Stream drops farmer application",
    "version": "1.0.0",
    "main": ".webpack/main",
    "scripts": {
        "start": "set CLEAR_CACHE=0 && electron-forge start",
        "start:fresh": "set CLEAR_CACHE=1 && electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish"
    },
    "author": {
        "name": "Soryn BÃ¤chli",
        "email": "soryn.baechli@gmail.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "./resources/icon.ico"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "platforms": [
                        "win64",
                        "win32"
                    ],
                    "config": {
                        "name": "drop-farmer"
                    }
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {
                        "debianArch": "arm64"
                    }
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/renderer/index.html",
                                    "js": "./src/renderer/renderer.tsx",
                                    "name": "main_window",
                                    "preload": {
                                        "js": "./src/main/preload.js"
                                    }
                                }
                            ]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.63",
        "@electron-forge/maker-deb": "^6.0.0-beta.63",
        "@electron-forge/maker-rpm": "^6.0.0-beta.63",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
        "@electron-forge/maker-zip": "^6.0.0-beta.63",
        "@electron-forge/plugin-webpack": "6.0.0-beta.63",
        "@types/auto-launch": "^5.0.2",
        "@types/node": "^18.6.1",
        "@vercel/webpack-asset-relocator-loader": "1.7.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.0.0",
        "electron": "19.0.9",
        "electron-devtools-installer": "^3.2.0",
        "fork-ts-checker-webpack-plugin": "^6.0.1",
        "node-loader": "^2.0.0",
        "sass": "^1.54.0",
        "sass-loader": "^13.0.2",
        "style-loader": "^3.0.0",
        "ts-loader": "^9.2.2",
        "typescript": "~4.5.4"
    },
    "dependencies": {
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "ansi-colors": "^4.1.3",
        "auto-launch": "^5.0.5",
        "cron-job-manager": "^2.3.1",
        "dayjs": "^1.11.4",
        "electron-squirrel-startup": "^1.0.0",
        "is-online": "^10.0.0",
        "puppeteer-core": "^16.0.0",
        "puppeteer-in-electron": "^3.0.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0",
        "react-switch": "^7.0.0",
        "timer-node": "^5.0.6",
        "uuid": "^8.3.2"
    }
}
